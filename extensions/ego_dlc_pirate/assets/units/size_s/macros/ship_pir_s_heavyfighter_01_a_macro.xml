<replace sel="//macros">
<macros>
  <macro name="ship_pir_s_heavyfighter_01_a_macro" class="ship_s">
    <component ref="ship_pir_s_heavyfighter_01" />
    <properties>
      <identification name="{20101,82901}" makerrace="argon" description="{20101,82911}" icon="ship_s_heavyfighter_01" />
      <software>
        <software ware="software_dockmk2" compatible="1" />
        <software ware="software_flightassistmk1" default="1" />
        <software ware="software_scannerlongrangemk1" default="1" />
        <software ware="software_scannerlongrangemk2" compatible="1" />
        <software ware="software_scannerobjectmk1" default="1" />
        <software ware="software_scannerobjectmk2" compatible="1" />
        <software ware="software_targetmk1" compatible="1" />
        <software ware="software_trademk1" compatible="1" />
      </software>
      <jerk>
        <forward accel="0.4" decel="0.3" ratio="3" />
        <forward_boost accel="0.4" ratio="3" />
        <forward_travel accel="0.4" decel="0.9" ratio="3" />
        <strafe value="0.7" />
        <angular value="0.7" />
      </jerk>
      <storage missile="30" />
      <hull max="9000" />
      <secrecy level="1" />
      <steeringcurve>
        <point position="1.01" value="1" />
        <point position="1.15" value="0.65" />
        <point position="1.4" value="0.4" />
        <point position="1.8" value="0.3" />
        <point position="2.25" value="0.12" />
      </steeringcurve>
      <purpose primary="fight" />
      <people capacity="2" />
      <sounds>
        <shipdetail ref="shipdetail_ship_03" />
      </sounds>
      <physics mass="10.353">
        <inertia pitch="3.8" yaw="3.8" roll="3.2" />
        <drag forward="4.2" reverse="13.765" horizontal="2.893" vertical="2.893" pitch="4.501" yaw="4.501" roll="4.501" />
      </physics>
      <thruster tags="small" />
      <ship type="heavyfighter" />
      <sound_occlusion inside="0.5" />
    </properties>
    <connections>
      <connection ref="con_cockpit">
        <macro ref="cockpit_gen_virtual_01_macro" connection="ship" />
      </connection>
      <connection ref="con_dock_xs">
        <macro ref="dock_gen_xs_ship_01_macro" connection="Connection_component" />
      </connection>
      <connection ref="con_shipstorage_xs_01">
        <macro ref="shipstorage_gen_xs_01_macro" connection="object" />
      </connection>
      <connection ref="con_storage01">
        <macro ref="storage_pir_s_heavyfighter_01_a_macro" connection="ShipConnection" />
      </connection>
    </connections>
  </macro>
</macros>
</replace>
