<diff> 
  <replace sel="//macros/macro[@name='ship_gen_s_fightingdrone_01_a_macro']"> 
  

  <macro name="ship_gen_s_fightingdrone_01_a_macro" class="ship_s">
    <component ref="ship_gen_s_fightingdrone_01" />
    <properties>
      <identification name="{20101,100401}" description="{20101,100411}" unit="1" icon="ship_s_drone" />
      <software>
        <software ware="software_dockmk2" compatible="1" />
        <software ware="software_flightassistmk1" default="1" />
        <software ware="software_scannerlongrangemk1" default="1" />
        <software ware="software_scannerlongrangemk2" compatible="1" />
        <software ware="software_scannerobjectmk1" default="1" />
        <software ware="software_scannerobjectmk2" compatible="1" />
        <software ware="software_targetmk1" default="1" />
        <software ware="software_trademk1" compatible="1" />
      </software>
      <jerk>
        <forward accel="1" decel="0.3" ratio="3" />
        <forward_boost accel="1" ratio="3" />
        <forward_travel accel="1" decel="0.9" ratio="3" />
        <strafe value="1" />
        <angular value="1" />
      </jerk>
      <storage countermeasure="0" />
      <hull max="1200" />
      <secrecy level="2" />
      <steeringcurve>
        <point position="1.01" value="1" />
        <point position="1.5" value="0.5" />
      </steeringcurve>
      <purpose primary="fight" />
      <physics mass="7.365">
        <inertia pitch="1.323" yaw="1.323" roll="1.059" />
        <drag forward="1.5" reverse="12.365" horizontal="6.494" vertical="6.494" pitch="3.239" yaw="3.239" roll="3.239" />
      </physics>
      <thruster tags="small" />
      <loadouts>
        <loadout id="default">
          <macros>
            <engine macro="engine_arg_s_combat_01_mk2_macro" path="../con_engine_01" />
            <weapon macro="weapon_gen_s_laser_01_mk1_macro" path="../con_weapon_01" optional="1" />
            <shield macro="ishield_gen_s_fightingdrone_01_a_macro" path="../con_ishield_01" optional="1" />
          </macros>
          <virtualmacros>
            <thruster macro="thruster_gen_s_combat_01_mk1_macro" />
          </virtualmacros>
        </loadout>
      </loadouts>
      <capture allow="0" />
      <ship type="smalldrone" />
    </properties>
    <connections>
      <connection ref="con_cockpit">
        <macro ref="cockpit_gen_invisible_01_macro" connection="ship" />
      </connection>
    </connections>
  </macro>

 </replace>
