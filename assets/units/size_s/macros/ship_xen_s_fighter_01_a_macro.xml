<diff> 
<replace sel="//macros/macro[@name='ship_xen_s_fighter_01_a_macro']"> 


  <macro name="ship_xen_s_fighter_01_a_macro" class="ship_s">
    <component ref="ship_xen_s_fighter_01" />
    <properties>
      <identification name="{20101,70101}" description="{20101,70111}" icon="ship_s_fight_01" />
      <software>
        <software ware="software_dockmk2" compatible="1" />
        <software ware="software_flightassistmk1" default="1" />
        <software ware="software_scannerlongrangemk1" default="1" />
        <software ware="software_scannerlongrangemk2" compatible="1" />
        <software ware="software_scannerobjectmk1" default="1" />
        <software ware="software_scannerobjectmk2" compatible="1" />
        <software ware="software_targetmk1" default="1" />
        <software ware="software_trademk1" compatible="1" />
      </software>
      <jerk>
        <forward accel="0.6" decel="0.3" ratio="3" />
        <forward_boost accel="0.6" ratio="3" />
        <forward_travel accel="0.6" decel="0.9" ratio="3" />
        <strafe value="1" />
        <angular value="1" />
      </jerk>
      <storage missile="20" />
      <hull max="3000" />
      <secrecy level="2" />
      <steeringcurve>
        <point position="1.01" value="1" />
        <point position="1.3" value="0.65" />
        <point position="2" value="0.35" />
        <point position="2.4" value="0.2" />
        <point position="3" value="0.15" />
      </steeringcurve>
      <purpose primary="fight" />
      <people capacity="0" />
      <physics mass="5.508">
        <inertia pitch="2.7" yaw="2.7" roll="2.1" />
        <drag forward="2.4" reverse="15.5" horizontal="4.3" vertical="4.3" pitch="3.2" yaw="3.2" roll="3.2" />
      </physics>
      <thruster tags="small" />
      <loadouts>
        <loadout id="default">
          <macros>
            <engine macro="engine_xen_s_combat_01_mk1_macro" path="../con_engine_01" />
            <engine macro="engine_xen_s_combat_01_mk1_macro" path="../con_engine_02" />
            <shield macro="shield_xen_s_standard_01_mk2_macro" path="../con_shield_01" optional="1" />
            <shield macro="ishield_xen_s_fighter_01_a_macro" path="../con_ishield_01" optional="1" />
            <weapon macro="weapon_xen_s_laser_01_mk1_macro" path="../con_primaryweapon_01" optional="1" />
            <weapon macro="weapon_xen_s_laser_01_mk1_macro" path="../con_primaryweapon_02" optional="1" />
          </macros>
          <ammunition>
            <ammunition macro="countermeasure_flares_01_macro" min="10" max="20" optional="1" />
          </ammunition>
          <virtualmacros>
            <thruster macro="thruster_gen_s_combat_01_mk1_macro" />
          </virtualmacros>
        </loadout>
      </loadouts>
      <capture allow="0" />
      <ship type="fighter" />
    </properties>
    <connections>
      <connection ref="con_cockpit">
        <macro ref="cockpit_gen_virtual_01_macro" connection="ship" />
      </connection>
    </connections>
  </macro>

</replace>
</diff> 
