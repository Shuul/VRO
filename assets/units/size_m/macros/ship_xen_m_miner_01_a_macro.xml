<replace sel="//macros/macro[@name='ship_xen_m_miner_01_a_macro']">


  <macro name="ship_xen_m_miner_01_a_macro" class="ship_m">
    <component ref="ship_xen_m_miner_01" />
    <properties>
      <identification name="{20101,70401}" description="{20101,70411}" icon="ship_m_mine_01" />
      <software>
        <software ware="software_dockmk2" compatible="1" />
        <software ware="software_flightassistmk1" default="1" />
        <software ware="software_scannerlongrangemk1" default="1" />
        <software ware="software_scannerlongrangemk2" compatible="1" />
        <software ware="software_scannerobjectmk1" default="1" />
        <software ware="software_scannerobjectmk2" compatible="1" />
        <software ware="software_targetmk1" compatible="1" />
        <software ware="software_trademk1" compatible="1" />
      </software>
      <jerk>
        <forward accel="0.4" decel="0.8" ratio="3" />
        <forward_boost accel="0.6" ratio="3" />
        <forward_travel accel="0.4" decel="0.9" ratio="3" />
        <strafe value="0.6" />
        <angular value="0.5" />
      </jerk>
      <hull max="33800" />
      <secrecy level="2" />
      <steeringcurve>
        <point position="1.01" value="1" />
        <point position="1.3" value="0.65" />
        <point position="1.6" value="0.45" />
        <point position="2" value="0.32" />
        <point position="2.3" value="0.2" />
        <point position="3" value="0.15" />
      </steeringcurve>
      <purpose primary="mine" />
      <people capacity="0" />
      <physics mass="49.951">
        <inertia pitch="10.5" yaw="10.5" roll="8" />
        <drag forward="3.85" reverse="28" horizontal="28" vertical="28" pitch="16" yaw="16" roll="16" />
      </physics>
      <thruster tags="medium" />
      <loadouts>
        <loadout id="default">
          <macros>
            <engine macro="engine_xen_m_travel_01_mk1_macro" path="../con_engine_01" />
            <shield macro="shield_xen_m_standard_01_mk1_macro" path="../con_shield_01" optional="1" />
            <shield macro="shield_xen_m_standard_01_mk1_macro" path="../con_shield_02" optional="1" />
            <shield macro="ishield_xen_m_miner_01_a_macro" path="../con_ishield_01" optional="1" />

            <turret macro="turret_xen_m_laser_01_mk1_macro" path="../con_turret_01" optional="1" />
            <weapon macro="weapon_xen_m_mining_01_mk1_macro" path="../con_primaryweapon_001" optional="1" />
          </macros>
          <ammunition>
            <ammunition macro="countermeasure_flares_01_macro" min="20" max="40" optional="1" />
          </ammunition>
          <virtualmacros>
            <thruster macro="thruster_gen_m_allround_01_mk1_macro" />
          </virtualmacros>
        </loadout>
      </loadouts>
      <capture allow="0" />
      <ship type="miner" />
    </properties>
    <connections>
      <connection ref="con_cockpit">
        <macro ref="cockpit_gen_virtual_01_macro" connection="ship" />
      </connection>
      <connection ref="con_storage01">
        <macro ref="storage_xen_m_miner_01_a_macro" connection="ShipConnection" />
      </connection>
    </connections>
  </macro>

</replace>
